  "scripts": {
    "start": "react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!react-markdown)/\"",
    "eject": "react-scripts eject",
    "lint:react": "eslint .",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s ./src",
    "build-storybook": "build-storybook -s ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@babel/plugin-syntax-class-static-block": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@storybook/addon-a11y": "^7.3.2",
    "@storybook/addon-actions": "^7.3.2",
    "@storybook/addon-controls": "^7.3.2",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/addons": "^7.3.2",
    "@storybook/builder-webpack5": "^7.3.2",
    "@storybook/node-logger": "^7.3.2",
    "@storybook/preset-create-react-app": "^7.3.2",
    "@storybook/react": "^7.3.2",
    "@storybook/theming": "^7.3.2",
    "axios-mock-adapter": "^1.20.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-storybook": "^0.5.13",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "prettier": "2.5.1",
    "react-refresh": "^0.11.0"
  },
  "resolutions": {
    "react-scripts/@svgr/webpack": "^6.5.1",
    "react-scripts/**/minimatch": "^3.0.5"
  }
}
